{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "typecheck": "tsc --build",
    "lint": "biome check --write ./",
    "format": "biome format --write ./",
    "preview": "vite preview",
    "postinstall": "pnpm exec simple-git-hooks"
  },
  "engines": {
    "node": "20.x",
    "pnpm": "^9.15.0"
  },
  "dependencies": {
    "apcach": "^0.6.4",
    "culori": "^3.2.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@svgr/webpack": "^8.1.0",
    "@types/culori": "^2.1.1",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "classnames": "^2.3.1",
    "globals": "^15.9.0",
    "nano-staged": "^0.8.0",
    "postcss": "^8.4.35",
    "simple-git-hooks": "^2.11.1",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "simple-git-hooks": {
    "pre-commit": "./node_modules/.bin/nano-staged",
    "pre-push": "pnpm lint"
  },
  "nano-staged": {
    "*.{js,cjs,mjs,jsx,ts,mts,tsx,json,css}": ["biome check --write"]
  }
}
